application {
    config {
        applicationType monolith
        authenticationType jwt
        baseName autocompleteentity
        buildTool maven
        cacheProvider ehcache
        clientFramework angularX
        clientPackageManager npm
        databaseType sql
        devDatabaseType h2Memory
        prodDatabaseType postgresql
        jhiPrefix aue
        languages [en, fr]
        nativeLanguage en
        packageName org.combo.autocompleteentity
    }

    entities *
}


entity Contact (aue_contact) {
    firstName String required,
    lastName String required,
    email String
}


entity Language (aue_language) {
    alpha3b String required maxlength(3),
    alpha2 String required maxlength(2)
    name String required,
    flag32	String,
    flag128 String,
    activated Boolean
}

relationship ManyToOne {
    Contact{language(name) required} to Language
}

service * with serviceClass
paginate * with pagination
filter *
